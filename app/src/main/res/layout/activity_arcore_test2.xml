<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activity.ARCoreActivity">

    <fragment android:name="com.google.ar.sceneform.ux.ArFragment"
              android:id="@+id/ux_fragment"
              android:layout_width="match_parent"
              android:layout_height="match_parent" tools:layout_editor_absoluteY="0dp"
              tools:layout_editor_absoluteX="0dp"/>
    <TextView
            android:id="@+id/countdownar"
            android:layout_width="87dp"
            android:layout_height="87dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:alpha="0.6"
            android:background="@drawable/circleshape_timerview"
            android:gravity="center"
            android:text="00:00"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            android:textStyle="bold"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/ux_fragment" app:layout_constraintVertical_bias="0.063"/>
    <ImageView
            android:layout_width="99dp"
            android:layout_height="90dp" app:srcCompat="@mipmap/background"
            android:id="@+id/shoppingitem1"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="1.0"
            android:alpha="0.5"/>
    <ImageView
            android:layout_width="99dp"
            android:layout_height="90dp" app:srcCompat="@mipmap/background"
            android:id="@+id/shoppingitem2"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="@+id/ux_fragment"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.665"
            app:layout_constraintVertical_bias="1.0"
            android:alpha="0.5"/>
    <ImageView
            android:layout_width="99dp"
            android:layout_height="90dp" app:srcCompat="@mipmap/background"
            android:id="@+id/shoppingitem3" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/ux_fragment" app:layout_constraintVertical_bias="1.0"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.331"
            android:alpha="0.5"/>
    <ImageView
            android:layout_width="99dp"
            android:layout_height="90dp" app:srcCompat="@mipmap/background"
            android:id="@+id/shoppingitem4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="1.0"
            android:alpha="0.5"/>
    <TextView
            android:text="x1"
            android:layout_width="18dp"
            android:layout_height="19dp"
            android:id="@+id/item4quantity"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/shoppingitem4" android:layout_marginBottom="63dp"
            app:layout_constraintBottom_toBottomOf="@+id/shoppingitem4"
            app:layout_constraintStart_toStartOf="@+id/shoppingitem4"
            app:layout_constraintEnd_toEndOf="@+id/shoppingitem4" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.917" app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="x1"
            android:layout_width="18dp"
            android:layout_height="19dp"
            android:id="@+id/item3quantity"
            app:layout_constraintTop_toTopOf="@+id/shoppingitem3"
            app:layout_constraintBottom_toBottomOf="@+id/shoppingitem3"
            app:layout_constraintEnd_toEndOf="@+id/shoppingitem3" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/shoppingitem4" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.97" app:layout_constraintVertical_bias="0.112"/>
    <TextView
            android:text="x1"
            android:layout_width="18dp"
            android:layout_height="19dp"
            android:id="@+id/item2quantity"
            app:layout_constraintStart_toEndOf="@+id/shoppingitem3"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="@+id/shoppingitem2"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/shoppingitem2"
            app:layout_constraintTop_toTopOf="@+id/shoppingitem2" app:layout_constraintHorizontal_bias="0.938"
            app:layout_constraintVertical_bias="0.112"/>
    <TextView
            android:text="x1"
            android:layout_width="18dp"
            android:layout_height="19dp"
            android:id="@+id/item1quantity"
            app:layout_constraintTop_toTopOf="@+id/shoppingitem1"
            app:layout_constraintBottom_toBottomOf="@+id/shoppingitem1"
            app:layout_constraintStart_toEndOf="@+id/shoppingitem2" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="@+id/shoppingitem1" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.112"/>

</androidx.constraintlayout.widget.ConstraintLayout>